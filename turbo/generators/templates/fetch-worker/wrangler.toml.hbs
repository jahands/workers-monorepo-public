name = "{{ slug name }}"
main = "src/index.ts"
compatibility_date = "2024-09-02"
compatibility_flags = ["nodejs_compat"]
workers_dev = false
logpush = true
routes = ["{{ slug name }}.example.com/*"]

[observability]
enabled = true
head_sampling_rate = 1

[placement]
mode = "smart"

[vars]
NAME = "{{ slug name }}"
ENVIRONMENT = "production"
AXIOM_DATASET = "workers"
AXIOM_DATASET_OTEL = "workers-otel"

